{
  "name": "starter-kit",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev db:studio",
    "dev:app": "turbo run dev",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "db:generate": "turbo run db:generate",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "turbo run db:studio",
    "db:schema": "pnpm --filter @repo/auth-server auth:generate",
    "db:setup": "pnpm db:schema && pnpm db:generate && pnpm db:migrate",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.3"
  }
}
