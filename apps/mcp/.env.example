# Base URL of the Resource Server
RESOURCE_BASE_URL=http://localhost:3010/api/v1/resource/

# HTTP server port for MCP protocol
MCP_SERVER_PORT=7411

# Optional: Shared JWT for stdio/local mode
# When running HTTP transport, JWT is passed through from client requests
# When running stdio transport (local), this JWT is used for all requests
MCP_AUTH_JWT=
